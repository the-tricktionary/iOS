# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'Tricktionary' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Tricktionary
	pod 'GoogleSignIn'
	pod 'SnapKit'
	pod 'ReactiveCocoa'
	pod 'RxSwift'
	pod 'YoutubePlayerView'
	pod 'SwiftChart'
	pod 'Charts'
	pod 'ChameleonFramework'
	pod 'LifetimeTracker'
	pod 'Swinject'
	pod 'SwiftMonkeyPaws'

  target 'TricktionaryTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'SwiftMonkey'
	pod 'SwiftMonkeyPaws'
	pod 'GoogleSignIn'
	pod 'SnapKit'
	pod 'ReactiveCocoa'
	pod 'RxSwift'
	pod 'YoutubePlayerView'
	pod 'SwiftChart'
	pod 'Charts'
	pod 'ChameleonFramework'
	pod 'LifetimeTracker'
	pod 'Swinject'
  end

  target 'TricktionaryUITests' do
    inherit! :search_paths
    # Pods for testing
	pod 'SwiftMonkeyPaws'
	pod 'GoogleSignIn'
	pod 'SnapKit'
	pod 'ReactiveCocoa'
	pod 'RxSwift'
	pod 'YoutubePlayerView'
	pod 'SwiftChart'
	pod 'Charts'
	pod 'ChameleonFramework'
	pod 'LifetimeTracker'
	pod 'Swinject'
	pod 'SwiftMonkey'
  end

end
